{
  "license": "MIT",
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:storybook": "STORYBOOK_PREFIX=storybook build-storybook -o ./public/storybook",
    "clean:next": "rimraf dist",
    "clean:storybook": "rimraf public/storybook",
    "cspell": "cspell --cache --config cspell.json '**'",
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3002",
    "dev:storybook": "start-storybook -p 6006 --ci",
    "eslint": "next lint --fix --config .eslintrc.js",
    "format": "prettier --ignore-path .prettierignore --config .prettierrc.json --cache --write .",
    "generate": "run-s generate:schema generate:types",
    "generate:schema": "get-graphql-schema http://localhost:5001/graphql > schema.graphql",
    "generate:types": "graphql-codegen --config codegen.yml",
    "kill-port:next": "kill-port 3002",
    "kill-port:storybook": "kill-port 6006",
    "lint": "run-p eslint cspell type-check",
    "postlint": "run-p format",
    "prebuild:next": "run-p clean:next",
    "prebuild:storybook": "run-s clean:storybook",
    "predev:next": "run-s kill-port:next",
    "predev:storybook": "run-s kill-port:storybook",
    "prod": "next start",
    "type-check": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/core": "^7.18.6",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/storybook-addon": "^4.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@faker-js/faker": "^7.3.0",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "browser-image-compression": "^2.0.0",
    "framer-motion": "^6.4.3",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "js-cookie": "^3.0.1",
    "next": "^12.1.6",
    "next-i18next": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-infinite-scroller": "^1.2.6",
    "sass": "^1.53.0",
    "sharp": "^0.30.7",
    "storybook-addon-apollo-client": "^4.0.12",
    "storybook-addon-next": "^1.6.7",
    "storybook-addon-turbo-build": "^1.1.0",
    "storybook-react-i18next": "^1.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.8.0",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.1",
    "@next/eslint-plugin-next": "^12.2.2",
    "@storybook/addon-a11y": "^6.5.9",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-infinite-scroller": "^1.2.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "get-graphql-schema": "^2.1.2",
    "tsconfig": "*"
  }
}
