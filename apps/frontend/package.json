{
  "license": "MIT",
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:storybook": "build-storybook -c .storybook -o ./public/storybook",
    "clean:copy-static": "rimraf .storybook/static",
    "clean:next": "rimraf dist",
    "clean:storybook": "rimraf public/storybook",
    "copy-static": "cpx -C 'public/static/**' .storybook/static",
    "cspell": "cspell '**/*'",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev:next http://localhost:3002 cypress",
    "e2e:headless": "start-server-and-test dev:next http://localhost:3002 cypress:headless",
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3002",
    "dev:storybook": "start-storybook -c .storybook -p 6006 --ci",
    "eslint": "next lint --fix",
    "format": "prettier --cache --write .",
    "generate": "run-s generate:schema generate:types",
    "generate:schema": "get-graphql-schema http://localhost:5001/graphql > schema.graphql",
    "generate:types": "graphql-codegen --config codegen.yml",
    "kill-port:next": "kill-port 3002",
    "kill-port:storybook": "kill-port 6006",
    "lint": "run-p eslint cspell type-check",
    "prebuild:next": "run-p clean:next",
    "prebuild:storybook": "run-s clean:storybook copy-static",
    "precopy-static": "run-p clean:copy-static",
    "predev:next": "run-s kill-port:next",
    "predev:storybook": "run-s kill-port:storybook copy-static",
    "prelint": "run-p format",
    "prod": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@babel/core": "^7.18.5",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/storybook-addon": "^4.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@faker-js/faker": "^7.3.0",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-infinite-scroller": "^1.2.3",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "browser-image-compression": "^2.0.0",
    "framer-motion": "^6.3.11",
    "graphql": "^16.5.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "js-cookie": "^3.0.1",
    "next": "^12.1.6",
    "next-i18next": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.32.2",
    "react-icons": "^4.4.0",
    "react-infinite-scroller": "^1.2.6",
    "sass": "^1.52.3",
    "sharp": "^0.30.6",
    "storybook-addon-apollo-client": "^4.0.11",
    "storybook-addon-next": "^1.6.6",
    "storybook-addon-turbo-build": "^1.1.0",
    "storybook-react-i18next": "^1.1.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@next/eslint-plugin-next": "^12.1.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cpx": "^1.5.0",
    "cspell": "^6.1.2",
    "cypress": "^10.3.0",
    "eslint": "^8.18.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "get-graphql-schema": "^2.1.2",
    "kill-port": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0"
  }
}
