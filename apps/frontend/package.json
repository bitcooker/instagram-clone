{
  "license": "MIT",
  "name": "frontend-package",
  "packageManager": "yarn@1.22.17",
  "version": "0.0.0",
  "prettier": {
    "trailingComma": "none",
    "quoteProps": "consistent"
  },
  "cspell": {
    "language": "en",
    "version": "0.2",
    "ignorePaths": [
      "package.json",
      "*.tsbuildinfo",
      "frontend-words.txt",
      "dist",
      "public/{storybook,static,favicon}",
      "*.yml",
      "src/generated/**/*"
    ],
    "words": [
      "chakra",
      "vercel",
      "Vercel",
      "Kenta",
      "subcomponents",
      "Chakra",
      "Apollographql",
      "msapplication",
      "signup",
      "camelcase",
      "Cloudinary",
      "CLOUDINARY"
    ]
  },
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:storybook": "build-storybook -c .storybook -o ./public/storybook",
    "clean:copy-static": "rimraf .storybook/static",
    "clean:next": "rimraf dist",
    "clean:storybook": "rimraf public/storybook",
    "copy-static": "cpx -C 'public/static/**' .storybook/static",
    "cspell": "cspell '**/*'",
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3002",
    "dev:storybook": "start-storybook -c .storybook -p 6006 --ci",
    "eslint": "TIMING=1 next lint --fix",
    "format": "prettier --write .",
    "generate": "run-s generate:schema generate:swr",
    "generate:schema": "get-graphql-schema http://localhost:5001/graphql > schema.graphql",
    "generate:swr": "graphql-codegen --config codegen.yml",
    "kill-port:next": "kill-port 3002",
    "kill-port:storybook": "kill-port 6006",
    "lint": "run-p eslint cspell type-check",
    "prebuild:next": "run-p clean:next",
    "prebuild:storybook": "run-s clean:storybook copy-static",
    "precopy-static": "run-p clean:copy-static",
    "predev:next": "run-s kill-port:next",
    "predev:storybook": "run-s kill-port:storybook copy-static",
    "prelint": "run-p format",
    "prod": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@chakra-ui/react": "^1.8.6",
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@storybook/addon-essentials": "^6.5.0-alpha.50",
    "@storybook/builder-webpack5": "^6.5.0-alpha.50",
    "@storybook/manager-webpack5": "^6.5.0-alpha.50",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.0-alpha.50",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.42",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.4",
    "browser-image-compression": "^1.0.17",
    "cookie": "^0.4.2",
    "css-loader": "^6.7.1",
    "framer-motion": "^6.2.8",
    "js-cookie": "^3.0.1",
    "next": "^12.1.0",
    "next-translate": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "sharp": "^0.30.3",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-addon-turbo-build": "^1.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@next/eslint-plugin-next": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cpx": "^1.5.0",
    "cspell": "^5.19.3",
    "eslint": "^8.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.3.0",
    "graphql-codegen-plugin-typescript-swr": "^0.8.1",
    "graphql-request": "^4.2.0",
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2"
  }
}
