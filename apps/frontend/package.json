{
  "license": "MIT",
  "name": "frontend-package",
  "packageManager": "yarn@1.22.17",
  "version": "0.0.0",
  "prettier": {
    "trailingComma": "none",
    "quoteProps": "consistent"
  },
  "cspell": {
    "language": "en",
    "version": "0.2",
    "ignorePaths": [
      "package.json",
      "*.tsbuildinfo",
      "frontend-words.txt",
      "dist",
      "public/{storybook,static}"
    ],
    "words": [
      "chakra",
      "vercel",
      "Vercel",
      "Kenta",
      "subcomponents",
      "Chakra"
    ]
  },
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:storybook": "build-storybook -c .storybook -o ./public/storybook",
    "clean:copy-static": "rimraf .storybook/static",
    "clean:next": "rimraf dist",
    "clean:storybook": "rimraf public/storybook",
    "copy-static": "cpx -C 'public/static/**' .storybook/static",
    "cspell": "cspell '**/*'",
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3002",
    "dev:storybook": "start-storybook -c .storybook -p 6006 --ci",
    "eslint": "TIMING=1 next lint --fix",
    "format": "prettier --write .",
    "kill-port:next": "kill-port 3002",
    "kill-port:storybook": "kill-port 6006",
    "lint": "run-p eslint cspell type-check",
    "prebuild:next": "run-p clean:next",
    "prebuild:storybook": "run-s clean:storybook copy-static",
    "precopy-static": "run-p clean:copy-static",
    "predev:next": "run-p kill-port:next",
    "predev:storybook": "run-s kill-port:storybook copy-static",
    "prelint": "run-p format",
    "prod": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@chakra-ui/react": "^1.8.6",
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@storybook/addon-essentials": "^6.5.0-alpha.50",
    "@storybook/builder-webpack5": "^6.5.0-alpha.50",
    "@storybook/manager-webpack5": "^6.5.0-alpha.50",
    "@storybook/react": "^6.5.0-alpha.50",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.42",
    "babel-loader": "^8.2.4",
    "framer-motion": "^6.2.8",
    "next": "^12.1.0",
    "next-i18next": "^10.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "sharp": "^0.30.3",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-addon-turbo-build": "^1.1.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cpx": "^1.5.0",
    "cspell": "^5.19.3",
    "eslint": "^8.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2"
  }
}
