{
  "license": "MIT",
  "name": "backend-package",
  "packageManager": "yarn@1.22.17",
  "version": "0.0.0",
  "prettier": {
    "trailingComma": "none",
    "quoteProps": "consistent"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "preset": "ts-jest",
    "rootDir": "./src"
  },
  "cspell": {
    "ignorePaths": [
      "package.json",
      "*.tsbuildinfo",
      "dist",
      "prisma/migrations"
    ],
    "language": "en",
    "version": "0.2",
    "words": [
      "nestjs",
      "datasource",
      "cuid",
      "tablename",
      "Fastify",
      "schemaname"
    ]
  },
  "scripts": {
    "build": "run-p build:*",
    "build:nest": "nest build",
    "clean:nest": "rimraf dist",
    "cspell": "cspell '**/*'",
    "dev": "run-p dev:nest",
    "dev:nest": "nest start --watch",
    "eslint": "TIMING=1 eslint 'src/**/*' --fix",
    "format": "prettier --write .",
    "kill-port:nest": "kill-port 5001",
    "lint": "run-p eslint cspell type-check",
    "prebuild:nest": "run-s clean:nest prisma:generate",
    "predev": "run-p prisma:generate",
    "predev:nest": "run-p kill-port:nest",
    "prelint": "run-p format",
    "pretest": "run-p prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prod": "node dist/main",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/cli": "^8.2.4",
    "@nestjs/common": "^8.4.2",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.2",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/throttler": "^2.0.1",
    "@prisma/client": "^3.11.0",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-jwt": "^3.0.6",
    "apollo-server-express": "^3.6.7",
    "bcrypt": "^5.0.1",
    "check-password-strength": "^2.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.3.0",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "react-icons": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "supertest": "^6.2.2",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@nestjs/testing": "^8.4.2",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jest": "^27.5.1",
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "prisma": "^3.11.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0"
  }
}
