{
  "dockerComposeFile": "docker-compose.yml",
  "forwardPorts": [8080],
  "name": "Instagram Clone",
  "postCreateCommand": "make clean && yarn install && make initdb",
  "service": "app",
  "workspaceFolder": "/workspace",
  "settings": {
    "activitusbar.activeColour": "#FF0461",
    "activitusbar.alignment": "Right",
    "activitusbar.inactiveColour": "#FF0461",
    "activitusbar.views": [
      { "codicon": "debug-restart", "name": "command.eslint.restart", "tooltip": "Restart ESlint Server" },
      { "codicon": "debug-restart", "name": "command.typescript.restartTsServer", "tooltip": "Restart TS Server" },
      { "codicon": "debug-restart", "name": "command.apollographql/reloadService", "tooltip": "Restart Apollo Server" }
    ],
    "colorize.languages": ["typescript"],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.workingDirectories": [{ "mode": "auto" }],
    "files.associations": { ".json": "jsonc" },
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.statusBar.enabled": false,
    "graphql-config.load.rootDir": "apps/frontend",
    "javascript.preferences.importModuleSpecifier": "relative",
    "material-icon-theme.activeIconPack": "nest",
    "prettier.configPath": ".devcontainer/.prettierrc.vscode.json",
    "prettier.ignorePath": ".devcontainer/.prettierignore.vscode",
    "prettier.requireConfig": true,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.preferences.jsxAttributeCompletionStyle": "none",
    "typescript.tsdk": "node_modules/typescript/lib"
  },
  "extensions": [
    "esbenp.prettier-vscode",
    "Exelord.git-commits",
    "albert.TabOut",
    "Fooxly.themeswitch",
    "richie5um2.vscode-sort-json",
    "dbaeumer.vscode-eslint",
    "Prisma.prisma",
    "pflannery.vscode-versionlens",
    "kamikillerto.vscode-colorize",
    "styled-components.vscode-styled-components",
    "ms-vscode.live-server",
    "Gruntfuggly.activitusbar",
    "formulahendry.auto-rename-tag",
    "apollographql.vscode-apollo",
    "eamodio.gitlens",
    "astro-build.astro-vscode",
    "bradlc.vscode-tailwindcss"
  ]
}
