{
  "dockerComposeFile": "docker-compose.yml",
  "forwardPorts": [8080],
  "name": "Instagram Clone",
  "postCreateCommand": "make clean && yarn install && make initdb",
  "service": "app",
  "workspaceFolder": "/workspace",
  "settings": {
    "colorize.languages": ["typescript"],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.workingDirectories": [
      {
        "mode": "auto"
      }
    ],
    "files.associations": {
      "*.json": "jsonc"
    },
    "graphql-config.load.rootDir": "apps/frontend",
    "javascript.preferences.importModuleSpecifier": "relative",
    "material-icon-theme.activeIconPack": "nest",
    "prettier.configPath": ".devcontainer/.prettierrc.vscode.json",
    "prettier.ignorePath": ".devcontainer/.prettierignore.vscode",
    "prettier.requireConfig": true,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.preferences.jsxAttributeCompletionStyle": "none",
    "typescript.tsdk": "node_modules/typescript/lib"
  },
  "extensions": [
    "esbenp.prettier-vscode",
    "Exelord.git-commits",
    "albert.TabOut",
    "Fooxly.themeswitch",
    "richie5um2.vscode-sort-json",
    "dbaeumer.vscode-eslint",
    "Prisma.prisma",
    "pflannery.vscode-versionlens",
    "kamikillerto.vscode-colorize",
    "styled-components.vscode-styled-components",
    "GraphQL.vscode-graphql"
  ]
}
